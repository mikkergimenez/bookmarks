h1 a
  color black

.add-field
  margin auto
  font-size 30px
  padding 5px
  padding-left .5em
  height 80px
  width 90%
  border 0
  background-color #fff

.bookmark-title
  font-size: 18px

.bookmark-list
  background-color: rgb(244, 244, 244)

.bookmark-field-container
  padding 0
  border-left 1px solid #e8e8e8
  border-right 1px solid #e8e8e8

.domain-muted
  color #999
  margin-left 1em
  font-size 16px

.title-field {
  font-size: 14px;
}

.bookmark-list
  background-color: rgb(244, 244, 244);


.header
  background-color #444

.container
  padding 1em 1.5em


.add-field
  margin auto
  font-size 30px
  padding 5px
  padding-left .5em
  height 60px
  width 80%
  border 0
  background-color #fafafa

.ql-container, .add-field-textarea
  margin auto
  top 5vh
  font-size 14px
  max-width 960px
  padding-left 0
  height 95vh
  border 0
  background-color white
  border-top 1px solid #eee
  left 1%
  right 1%

.add-field:hover
  color black

.add-field-submit
  height: 60px
  margin-top: -12px
  border-radius: 0px 3px 3px 0px

body a
  font-family 'Inconsolata', monospace
  color #68B3AC

.card img {
  width: 100%;
}

.card {
  font-family: 'Inconsolata', monospace;
  border: 0;
  border-radius: 0px;
  border-bottom: 1px solid rgba(0,0,0,.125);
}

.card a.actions {
  visibility:hidden;
}

.card:hover a.actions {
 visibility:visible;
}

.change-show {
  font-size: 18px;
}

div.icon-set > span {
  display: inline-block;
  margin-left: 1em;
}



